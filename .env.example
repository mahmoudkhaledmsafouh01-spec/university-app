# Copy this file to .env (or .env.local) and adjust as needed
# Default connection points to the docker-compose Postgres service
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/universitydb"

# Used by NextAuth for signing cookies; replace with a strong secret in production
NEXTAUTH_SECRET="development-nextauth-secret"
# Base URL for your Next.js app (required by NextAuth)
NEXTAUTH_URL="http://localhost:3000"